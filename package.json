{
  "name": "br-angular-components-library",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:icon-modules && ng build",
    "build:icon-modules": "ts-node --project bin/tsconfig.json bin/create-icon-modules.ts",
    "test": "ng test",
    "lint": "ng lint --fix",
    "format": "prettier --write",
    "prepare": "husky install",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook:serve": "npm run docs:json && start-storybook -p 6006",
    "storybook:build": "npm run docs:json && build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.1",
    "@angular/common": "~12.2.1",
    "@angular/compiler": "~12.2.1",
    "@angular/core": "~12.2.1",
    "@angular/forms": "~12.2.1",
    "@angular/platform-browser": "~12.2.1",
    "@angular/platform-browser-dynamic": "~12.2.1",
    "@angular/router": "~12.2.1",
    "angular-svg-icon": "^12.0.0",
    "ng-inline-svg": "^13.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^12.1.1",
    "@angular-devkit/build-angular": "~12.2.1",
    "@angular-eslint/builder": "12.3.1",
    "@angular-eslint/eslint-plugin": "12.3.1",
    "@angular-eslint/eslint-plugin-template": "12.3.1",
    "@angular-eslint/schematics": "12.3.1",
    "@angular-eslint/template-parser": "12.3.1",
    "@angular/cli": "~12.2.1",
    "@angular/compiler-cli": "~12.2.1",
    "@angular/elements": "^12.2.1",
    "@babel/core": "^7.15.0",
    "@compodoc/compodoc": "^1.1.14",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/angular": "^6.3.7",
    "@storybook/builder-webpack5": "^6.3.7",
    "@storybook/manager-webpack5": "^6.3.7",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^14.17.5",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "@webcomponents/custom-elements": "^1.5.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-mocha-reporter": "^2.2.5",
    "lint-staged": "^12.3.2",
    "ng-packagr": "^12.2.0",
    "prettier": "2.3.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "protractor": "~7.0.0",
    "ts-node": "~9.1.1",
    "typescript": "~4.3.5"
  },
  "lint-staged": {
    "{/projects/**/src,src}/**/*.{ts,tsx,json}": [
      "prettier --write",
      "ng lint --fix"
    ],
    "{projects/**/src,src}/**/*.{css,scss,sass,less}": [
      "prettier --write"
    ],
    "!({projects/**/src/**/*,src/**/*}).{css,scss,sass,less,ts,tsx,json}": "prettier --write",
    "*.{html,md,yaml,js}": "prettier --write"
  }
}
